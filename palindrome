#include <stdio.h>
#include<string.h>
int main() {
    char str[10]="malayalam";
	int i=0;
	int t=0;
	int j=strlen(str)-1;
	while(j>i)
	{
	    if(str[i]==str[j])
	    {
	       t=1; 
	       i++;
	       j--;
	       
	    }
	    else
	    {
	        printf(" is not  palindrome ");
	        t=0;
	        break;
	    }
	}
	if(t)
	    printf("palindromr");
	return 0;
}
