#include <stdio.h>
#include<string.h>
int main() {
    char str[10]="malayalam";
	int i=0;
	int j=strlen(str)-1;
	while(j>i)
	{
	    if(str[i++]!=str[j--])
	    {
	        printf("%s not palindrome",str);
	        break;
	    }
	    else
	    {
	        printf(" is palindrome %s",str);
	        break;
	    }
	}
	return 0;
}
