a=list(map(int,input().split()))
b=a[2]
c=[1,3,5,7,8,10,12]
d=[4,6,9,11]
e=[2]
if a[1] in c:
    if(a[0]<=31):
        print(1)
    else:
         print(-1)
elif a[1] in d:
    if(a[0]<=30):
         print(1)
    else:
        print(-1)
elif a[1] in e:
    if(b%400==0 or (b%4==0 and b%100!=0)):
        if(a[0]<=29):
            print(1)
        else:
            print(-1)
    else:
        print(-1)
else:
    print(-1)




