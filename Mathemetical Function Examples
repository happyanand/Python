import math
Print(math.ceil(2.9)).  ====3
Print(math.floor(2.9)).   ===2


is_hot=True

If is_hot:
      Print("Hot")
Print("good") ======Hot//////good

is_hot=False

If is_hot:
      Print("Hot")
Print("good") ======good


is_hot=False

If is_hot:
      Print("Hot")
Else:
      Print("cold")
Print("good") ======cold good
